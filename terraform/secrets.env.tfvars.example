aws_region     = "us-west-2"
environment    = ""

api_env_vars = {
  "ADMIN_EMAIL"             = "admin@example.com",
  "DB_ENGINE_VERSION"       = "13.4",
  "DB_INSTANCE_CLASS"       = "db.t4g.small",
  "DD_AGENT_MAJOR_VERSION"  = "7",
  "DD_AGENT_MINOR_VERSION"  = "",
  "HEALTHCHECK_HEAP_MAX_MB" = "300",
  "HEALTHCHECK_RSS_MAX_MB"  = "300",
  "NODE_ENV"                = "DEVELOP",
  "NO_COLOR"                = "true",
  "SERVER_PORT"             = "3001",
  "TYPEORM_CONNECTION"      = "postgres",
  "TYPEORM_PORT"            = "5432",
  "TYPEORM_SYNCHRONIZE"     = "true"
}

api_secrets = {
  "ADMIN_ADDRESS"    = "",
