  "DATADOG_API_KEY" = ""
}

admin_env_vars = {
  "ADMIN_EMAIL"        = "admin@example.com",
  "NODE_ENV"           = "ADMIN",
  "NO_COLOR"           = "true",
  "SERVER_PORT"        = "3001",
  "TYPEORM_CONNECTION" = "postgres",
  "TYPEORM_PORT"       = "5432"
}

admin_secrets = {
  "ADMIN_EMAIL"      = "",
